# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Admin Authentication
ADMIN_PASSWORD=your-secure-admin-password-here

# Cron Job Security
CRON_SECRET=your-cron-secret-here

# Vercel Deploy Hook (optional - for auto-deployment after photo sync)
VERCEL_DEPLOY_HOOK_URL=https://api.vercel.com/v1/integrations/deploy/your-hook

# External Photo Sources (optional)
GOOGLE_DRIVE_API_KEY=your-google-drive-api-key
GOOGLE_DRIVE_FOLDER_URL=https://drive.google.com/drive/folders/your-folder-id
DROPBOX_ACCESS_TOKEN=your-dropbox-token
DROPBOX_FOLDER_URL=https://www.dropbox.com/sh/your-folder
EXTERNAL_PHOTO_URLS=https://example.com/photo1.jpg,https://example.com/photo2.jpg
